<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rzoa31imvnod"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rrf8w89zbjn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:paddingVertical="12dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rvejwb1i8xtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="@drawable/cr32beff1f5"
                    android:paddingVertical="10dp"
                    android:layout_marginHorizontal="16dp"
                    android:orientation="horizontal">
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/r61c0ix98fiw"
                        app:shapeAppearanceOverlay="@style/cr32"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="8dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rzplw0l2klr"
                        android:textColor="#A09CAB"
                        android:textSize="16sp"
                        android:layout_marginEnd="8dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Search for restaurants"
                        />
                    <LinearLayout
                        android:id="@+id/rntz0x5lcr2"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:orientation="vertical">
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rgtjfuz41pag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:paddingVertical="8dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r9vfeaqqscxf"
                    android:textColor="#1C1B1F"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:layout_marginHorizontal="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Discover Local Eats"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r3vnz5as0p3i"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="12dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/rat4mzybcia9"
                    android:layout_width="match_parent"
                    android:layout_height="219dp"
                    android:layout_marginHorizontal="16dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/pizza" />

                <TextView
                    android:id="@+id/rex2lpthvns5"
                    android:textColor="#1C1B1F"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="2dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Pizza Paradise"
                    />
                <TextView
                    android:id="@+id/rq20pxg6du1"
                    android:textColor="#A09CAB"
                    android:textSize="14sp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Grab the best slices in town!"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r27bq8tsuzt0i"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="12dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/r4njl38fgmwr"
                    android:layout_width="match_parent"
                    android:layout_height="219dp"
                    android:layout_marginHorizontal="16dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/sushi" />
                <TextView
                    android:id="@+id/rpefuyzh6sp"
                    android:textColor="#1C1B1F"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="2dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Sushi Central"
                    />
                <TextView
                    android:id="@+id/rnlsdnreitsh"
                    android:textColor="#A09CAB"
                    android:textSize="14sp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Delicious fresh sushi rolls!"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rw5ydawqxlia"
                android:layout_width="match_parent"
                android:layout_height="62dp"
                android:background="#FFFFFF"
                android:gravity="center_horizontal"
                android:orientation="vertical" />
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:id="@+id/r9b16ln30c8"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:paddingVertical="8dp"
        android:paddingHorizontal="36dp"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/rev0yqxv7b7c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingHorizontal="8dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/r4uatdyh3r72"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginBottom="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/outline_home_24" />
            <TextView
                android:id="@+id/r5gbv1a20iud"
                android:textColor="#1C1B1F"
                android:textSize="11sp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Home"
                />
        </LinearLayout>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            />
        <LinearLayout
            android:id="@+id/ra9iazaxowe9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingHorizontal="6dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/rfrve8moyq0t"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginBottom="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/outline_person_24" />
            <TextView
                android:id="@+id/rw6282k4x9n"
                android:textColor="#A09CAB"
                android:textSize="11sp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Profile"
                />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>