<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/rsrhl3ikwee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:padding="16dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/rvc092u6o309"
            android:textColor="#1C1B1F"
            android:textSize="17sp"
            android:textStyle="bold"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="9:41 AM" />

        <ImageView
            android:id="@+id/r9aud7zm9ql"
            android:layout_width="20dp"
            android:layout_height="13dp"
            android:layout_marginEnd="9dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY" />

        <ImageView
            android:id="@+id/ra1791m1xmf"
            android:layout_width="18dp"
            android:layout_height="13dp"
            android:layout_marginEnd="7dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY" />

        <ImageView
            android:id="@+id/rohznriq4b3n"
            android:layout_width="25dp"
            android:layout_height="11dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/r76ym3t39isr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView
            android:id="@+id/r5xooajwdhup"
            android:textColor="#1C1B1F"
            android:textSize="32sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Welcome Back!" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rpnmtq0ny8p"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/rj0d4vnfn78"
            android:layout_width="188dp"
            android:layout_height="182dp"
            android:scaleType="fitXY"
            android:src="@drawable/scooter" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rh9xiv2kbl9b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView
            android:id="@+id/r4fliv5x2e4a"
            android:textColor="#1C1B1F"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Log in to explore your favorite meals" />
    </LinearLayout>

    <EditText
        android:id="@+id/rf01tffw7uu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:background="@drawable/cr12beff1f5"
        android:hint="Email"
        android:inputType="text"
        android:minHeight="48dp"
        android:padding="12dp"
        android:textColor="#A09CAB"
        android:textSize="16sp" />

    <LinearLayout
        android:id="@+id/r0qeuhynz86s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:background="@drawable/cr12beff1f5"
        android:padding="12dp"
        android:layout_margin="12dp"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/rm6oc9gx8luo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:hint="Password"
            android:inputType="text"
            android:textColor="#A09CAB"
            android:textSize="16sp"
            tools:ignore="TouchTargetSizeCheck" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/rmnvbthj78h"
            app:shapeAppearanceOverlay="@style/cr12"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/r16rotuu1w09"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:background="@drawable/cr12b55b2ff"
        android:padding="16dp"
        android:layout_margin="16dp"
        android:clickable="true"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:id="@+id/ru86yjjt7nqr"
            android:textColor="#FFFFFF"
            android:textSize="32sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Log In" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rd8t92jzq6ov"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="16dp">

        <TextView
            android:id="@+id/rxt22mxtampf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/cr12beff1f5"
            android:padding="14dp"
            android:text="Login with Google"
            android:textColor="#97AF56"
            android:textSize="16sp"
            android:textStyle="bold"
            android:gravity="center"/>

        <TextView
            android:id="@+id/rtvnm9ndfub"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/cr12beff1f5"
            android:padding="14dp"
            android:text="Login with Facebook"
            android:textColor="#45B8F5"
            android:textSize="16sp"
            android:textStyle="bold"
            android:gravity="center"/>
    </LinearLayout>
    <TextView
        android:id="@+id/rxcbijf96zbi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forgot Password?"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:textSize="14sp"
        android:textColor="#1C1B1F"/>
</LinearLayout>